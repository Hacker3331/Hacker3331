local OrionLib = loadstring(game:HttpGet(('https://raw.githubusercontent.com/shlexware/Orion/main/source')))()
local Window = OrionLib:MakeWindow({Name = "Hacker Hub | ðŸ’¥ Flag Wars!", HidePremium = false, IntroText = "Welcome to Hacker Hub", SaveConfig = false, ConfigFolder = "OrionTest"})

--Values

_G.gunMod = true
_G.circleAim = true
_G.infiniteAmmo = true
_G.noClip = true
_G.espPlayer = true
_G.reJoin = true
_G.infiniteYield = true
_G.silentAim = true

--Functions

function gunMod()
	while _G.gunMod == true do
		OrionLib:MakeNotification({
			Name = "Credit",
			Content = "Credits : DylanIsKing",
			Image = "rbxassetid://4483345998",
			Time = 8
		})
	    loadstring(game:HttpGet("https://raw.githubusercontent.com/DylanIsAKingL/Lua-Scripts/main/flag-wars-gunmod.lua"))()
	end
   end

function circleAim()
	while _G.circleAim == true do
	    loadstring(game:HttpGet("https://pastebin.com/raw/gMeAWLfS"))()
	end
   end

function silentAim()
    while _G.silentAim == true do
        loadstring(game:HttpGet("https://pastebin.com/raw/R6wdnuXL"))()
    end
   end

function infiniteAmmo()
    while _G.infiniteAmmo == true do
	loadstring(game:HttpGet("https://pastebin.com/raw/wYPRhavC"))()
    end
   end

function noClip()
    while _G.noClip == true do
        loadstring(game:HttpGet("https://pastebin.com/raw/vgUPcFNh"))()
    end
   end

