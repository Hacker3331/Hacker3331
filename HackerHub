})

-- Toggles

MainTab:AddButton({
    Name = "Gun Mod",
    Callback = function()
            print("button pressed")
    end    
})

MiscTab:AddButton({
    Name = "Rejoin The Server",
    Callback = function()
      	    print("button pressed")
    end    
})

MiscTab:AddButton({
    Name = "Infinite Yield",
    Callback = function()
      	     print("button pressed")
    end    
})

AimTab:AddToggle({
   Name = "Circle Aim",
   Default = false,
   Callback = function(Value)
       _G.circleAim = Value
       circleAim()
   end    
})

MainTab:AddToggle({
    Name = "Infinite Ammo",
    Default = false,
    Callback = function(Value)
        _G.infiniteAmmo = Value
	infiniteAmmo()
    end    
})

