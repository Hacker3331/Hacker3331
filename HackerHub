		})
		loadstring(game:HttpGet('https://raw.githubusercontent.com/Hypeploits/Hypeploits/main/Flag%20Wars'))()
	end
   end

function reJoin()
    while _G.reJoin == true do
        loadstring(game:HttpGet("https://pastebin.com/raw/CJVynp6g"))()
    end
   end

function infiniteYield()
	while _G.infiniteYield == true do
		OrionLib:MakeNotification({
			Name = "Credit",
